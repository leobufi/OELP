<div class="home-section min-h top">
  <div class="logo-wrapper">
    <div class="logo animate__animated animate__pulse animate__infinite">
      <%= image_tag("LOGO/OELP_logo.svg") %>
    </div>
  </div>
  <div class="sun animate__animated animate__pulse animate__infinite">
    <%= image_tag("SVG/OELP_soleil.svg") %>
  </div>
  <div class="scene-1">
    <div class="parasol-1">
      <%= image_tag("SVG/OELP_parasol_jaune.png") %>
    </div>
    <div class="ombre-1">
      <%= image_tag("SVG/OELP_ombre_parasol_4.svg") %>
    </div>
    <div class="serviette-2">
      <%= image_tag("SVG/OELP_serviette_1.svg") %>
    </div>
    <div class="glaciere-2">
      <%= image_tag("SVG/OELP_glacière2.svg") %>
    </div>
    <div class="canette-1">
      <%= image_tag("SVG/OELP_canette_2.svg") %>
    </div>
    <div class="perso">
      <%= image_tag("SVG/OELP_perso2.svg") %>
    </div>
  </div>
  <div class="scene-2">
    <div class="transat-1">
      <%= image_tag("SVG/OELP_ombre&tranzat.svg") %>
    </div>
  </div>
  <div class="scene-3">
    <div class="parasol-2">
      <%= image_tag("SVG/OELP_parasol_vert.svg") %>
    </div>
    <div class="ombre-parasol-2">
      <%= image_tag("SVG/OELP_ombre_parasol_2.svg") %>
    </div>
    <div class="serviette-1">
      <%= image_tag("SVG/OELP_serviette_2.svg") %>
    </div>
    <div class="glaciere-1">
      <%= image_tag("SVG/OELP_glacière.svg") %>
    </div>
    <div class="perso">
      <%= image_tag("SVG/OELP_perso1.svg") %>
    </div>
  </div>
  <div class="scene-4">
    <div class="parasol-3">
      <%= image_tag("SVG/OELP_parasol_rose.svg") %>
    </div>
    <div class="ombre-3">
      <%= image_tag("SVG/OELP_ombre_parasol_3.svg") %>
    </div>
    <div class="baguette">
      <%= image_tag("SVG/OELP_baguette.svg") %>
    </div>
    <div class="serviette">
      <%= image_tag("SVG/OELP_serviette_3.svg") %>
    </div>
  </div>
  <div class="mouettes">
    <div class="mouette-2">
      <div class="mouette-anim  animate__animated animate__pulse animate__infinite">
        <%= image_tag("SVG/OELP_mouette_vol_1.svg") %>
      </div>
      <div class="ombre mouette-anim animate__animated animate__pulse animate__infinite">
        <%= image_tag("SVG/OELP_ombre_mouette_vol_1.svg") %>
      </div>
    </div>
    <div class="mouette-3" data-onload-target="mouette1">
      <div class="mouette mouette-anim animate__animated animate__pulse animate__infinite" data-onload-target="mouette2">
        <%= image_tag("SVG/OELP_mouette1.svg") %>
      </div>
      <div class="ombre mouette-anim animate__animated animate__pulse animate__infinite" data-onload-target="mouette2">
        <%= image_tag("SVG/OELP_ombre_mouette_vol_2.svg") %>
      </div>
    </div>
    <div class="mouette-4">
      <div class="mouette-anim animate__animated animate__pulse animate__infinite">
        <%= image_tag("SVG/OELP_mouette_vol_1.svg") %>
      </div>
    </div>
    <div class="mouette-5">
      <%= image_tag("SVG/OELP_mouette_posee_ombre.svg") %>
    </div>
    <div class="mouette-6">
      <%= image_tag("SVG/OELP_mouette_posee_ombre.svg") %>
    </div>
  </div>
  <div class="wave">
    <%= image_tag("OELP_vague_jaune_up.svg") %>
  </div>
</div>
